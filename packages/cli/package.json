{
  "name": "@cartguard/cli",
  "version": "0.1.0",
  "type": "module",
  "description": "CartGuard command line interface",
  "license": "MIT",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "cartguard": "./dist/src/bin/cartguard.js"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "pnpm run build && node --test dist/test/*.test.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cartguard/ai": "workspace:*",
    "@cartguard/engine": "workspace:*",
    "@cartguard/spec": "workspace:*"
  }
}
